// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ClubResult {
  id               Int       @id @default(autoincrement())
  clubId           String
  wonGamesCount    Int
  drawGamesCount   Int
  lostGamesCount   Int
  updatedAt        DateTime  @updatedAt
}
